{
  "name": "@dollarsign/utils",
  "version": "0.0.0-development",
  "description": "Utilities for the Node.js",
  "author": "Dollarsign",
  "license": "MIT",
  "readmeFilename": "README.md",
  "main": "dist/index",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "keywords": [
    "utils",
    "Utilities"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dollarsignteam/node-utils.git"
  },
  "bugs": "https://github.com/dollarsignteam/node-utils/issues",
  "scripts": {
    "prebuild": "rimraf dist/*",
    "build": "tsc && tsc-alias",
    "test": "jest --forceExit --detectOpenHandles",
    "test:cov": "npm run test -- --verbose --coverage",
    "test:watch": "npm run test -- --coverage --watchAll",
    "format": "prettier src/**/*.ts --write",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/ --fix",
    "type-check": "tsc-files --noEmit",
    "fix": "tsc --noEmit && yarn format && yarn lint",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "docs": "typedoc --theme default src/index.ts"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "dependencies": {
    "safe-stable-stringify": "^1.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/jest": "^26.0.23",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "eslint-plugin-typescript-sort-keys": "^1.7.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "node-gyp": "^8.1.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.4",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "tsc-alias": "^1.3.7",
    "tsc-files": "^1.1.2",
    "tsconfig-paths": "^3.10.1",
    "typedoc": "^0.21.2",
    "typescript": "^4.3.5"
  }
}
